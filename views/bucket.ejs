<!DOCTYPE html>
<html lang="ja">
<head>
  <meta charset="utf-8">
  <link href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-WskhaSGFgHYWDcbwN70/dfYBj47jz9qbsMId/iRN3ewGhXQFZCSftd1LZCfmhktB" crossorigin="anonymous">
  <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.1/js/bootstrap.min.js"
          integrity="sha384-smHYKdLADwkXOn1EmN1qk/HfnUcbVRZyYmZ4qpPea6sjB/pTJ0euyQp0Mk8ck+5T"
          crossorigin="anonymous"></script>
</head>

<body>
<div class="container">
  <h1>Cloud Object Storage PoC</h1>
  <a href="/">Home</a>

  <h2>Objects</h2>
  <form method="post" action="/<%= data.Name %>" enctype="multipart/form-data">
    <div class="input-group">
      <input type="file" class="mt-1" name="upload-file">
      <button type="submit" class="btn btn-primary">Upload</button>
    </div>
  </form>

  <ol class="mt-2">
    <%
    const list = data && data.Contents ? data.Contents : [];
    list.forEach(item => {
    %>

    <li class="mt-2">
      <a href="/<%= data.Name %>/<%= item.Key %>" target="_blank"><%= item.Key %></a>
      <button onclick="location.href='/<%= data.Name %>/<%= item.Key %>/delete'" type="submit"
              class="btn btn-outline-danger btn-sm ml-2">Delete
      </button>
    </li>

    <%
    });
    %>
  </ol>

  <hr>
  <h2>Result listObjects</h2>
  <pre><%= JSON.stringify(data, undefined, 2) %></pre>


</div>

</body>

</html>